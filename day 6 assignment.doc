Q1.Write a funtion to find the maximum element in the stack.

void push(int x)
{
mainstack.push(x);
if(mainstack.size()==1)
maxstack.push(x);
}
if(x>maxstack.top())
maxstack.push(x);
else
maxstack.push(maxstack.top());
}

int getmax()
{
return maxstack.top();
}


